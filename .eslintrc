{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "react-app",
    "prettier",
    "prettier/react",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "browser": true
  },
  "globals": {},
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "prettier/prettier": 1,
    "@typescript-eslint/member-delimiter-style": 0
  }
}
